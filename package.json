{
  "name": "100-coaches",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "codegen": "graphql-codegen --config graphql-codegen.ts",
    "generate-barrels": "barrelsby --config barrelsby.json",
    "generate-barrels:watch": "chokidar \"src/**/*.{js,jsx,ts,tsx}\" -i \"src/**/index.{js,jsx,ts,tsx}\" -c 'npm run generate-barrels'",
    "dev": "concurrently \"next dev -p 3000\" \"npm run codegen -- --watch\" \"npm run generate-barrels:watch\" ",
    "prebuild": "npm run generate-barrels && npm run codegen",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "npx eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md,scss,css}\"",
    "gatsby-to-next": "node codemods/gatsby-to-next.mjs src/features src/app && npm run format",
    "split-gql": "node codemods/split-gql-from-components.mjs src/features && npm run format"
  },
  "dependencies": {
    "@datocms/cda-client": "^0.2.7",
    "@gsap/react": "^2.1.2",
    "@libsql/client": "0.15",
    "@next/third-parties": "^16.0.1",
    "date-fns": "^4.1.0",
    "datocms-structured-text-to-plain-text": "^5.1.6",
    "datocms-structured-text-utils": "^5.1.6",
    "graphql-tag": "^2.12.6",
    "html-react-parser": "^5.2.7",
    "i18n-iso-countries": "^7.14.0",
    "include-media": "^2.0.0",
    "libphonenumber-js": "^1.12.25",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^8.0.0",
    "next": "16.0.1",
    "next-sitemap": "^4.2.3",
    "node-downloader-helper": "^2.1.9",
    "normalize.css": "^8.0.1",
    "react": "^19",
    "react-datocms": "^7.2.3",
    "react-dom": "^19",
    "react-icons": "^5.5.0",
    "react-inlinesvg": "^4",
    "react-intersection-observer": "^10.0.0",
    "react-player": "^3.3.3",
    "sass": "^1.93.3",
    "smoothscroll-polyfill": "^0.4.4"
  },
  "devDependencies": {
    "@datocms/cli": "^3.1.7",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@parcel/watcher": "^2.5.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/smoothscroll-polyfill": "^0.3.4",
    "barrelsby": "^2.8.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.0",
    "eslint-config-next": "^16.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-unused-imports": "^4.3.0",
    "fs-extra": "^11.3.2",
    "globals": "^16.5.0",
    "globby": "^15.0.0",
    "graphql": "^16.12.0",
    "jscodeshift": "^17.3.0",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.46.3"
  }
}
